{
  "frases": {
    "nominalizacao": "/ensepro/configuracoes/nominalizacao.json",
    "regex": {
      "termo_relevante": ":(v-|n|prop)",
      "palavra_adjetivo": ":adj$",
      "palavra_substantivo": ":n$",
      "palavra_preposicao": ":prp$",
      "palavra_substantivo_proprio": ":prop$",
      "palavra_verbo": "v-",
      "voz_passiva": "^fApass:pp$"
    },
    "verbos_de_ligacao": [
      "ser",
      "estar",
      "permanecer",
      "continuar",
      "parecer",
      "tornar",
      "ficar",
      "possuir",
      "ter",
      "precisar"
    ],
    "termos_relevantes": {
      "ignorar": [
        "existir",
        "vir"
      ]
    }
  },
  "sinonimos": {
    "linguagens": [
      "por",
      "eng"
    ]
  },
  "cbc": {
    "lca_size": 500,
    "slm1_factor": 2000,
    "slm1_factor_only_l1": false,
    "numero_respostas": 30,
    "path_answer_generator": "C:\\_ensepro\\ensepro-answer-generator\\jars\\ensepro-answer-generator-size-slm1.jar",
    "nivel_answer_generator": 2,
    "threads_answer_generator": 8,
    "threshold_predicate": 0.3,
    "threshold_answer": 0.3,
    "pesos": {
      "classes": {
        "substantivo_proprio": 10,
        "substantivo_comum": 2,
        "substantivo_comum_sinonimo": 1.8,
        "verbo": 3,
        "verbo_sinonimo": 1,
        "verbo_nomilizado": 2.8,
        "verbo_nomilizado_sinonimo": 1,
        "adjetivo": 1
      },
      "metricas": {
        "m1": {
          "peso": 1,
          "policy": "BEST_MATCH"
        },
        "m2": {
          "peso": 1,
          "policy": "BEST_MATCH"
        },
        "m3": {
          "peso": 1,
          "policy": "BEST_MATCH"
        }
      }
    },
    "termos_tipos_frases": {
      "alguem": [],
      "algum": [],
      "eh_um": [],
      "em_que": [
        "tipo",
        "type"
      ],
      "quanto": [],
      "quando": [
        "data",
        "date"
      ],
      "o_que": [],
      "qual": [],
      "quem": [],
      "onde": [],
      "consulta": []
    },
    "resultado_resumido": false,
    "remover_variaveis": true
  },
  "servidores": {
    "palavras": {
      "endpoint": "http://host.docker.internal",
      "porta": "8099",
      "servicos": {
        "analisar_frase": "/palavras/analisar/"
      }
    },
    "word_embedding": {
      "endpoint": "http://host.docker.internal",
      "porta": "8098",
      "servicos": {
        "word_embedding": "/similarity"
      }
    },
    "elastic_search": {
      "endpoint": "host.docker.internal",
      "porta": "9200",
      "settings": {
        "index_name": "ensepro_triplas",
        "index_type": "ensepro_tripla",
        "username": "",
        "password": ""
      }
    },
    "dbpedia_spotlight": {
      "endpoint": "http://host.docker.internal:2228",
      "confiancas": [
        0.8,
        0.7,
        0.5,
        0.3
      ],
      "servicos": {
        "spotlight": "/rest/annotate/"
      }
    },
    "knowledge_graph_search": {
      "endpoint": "https://kgsearch.googleapis.com/v1/entities",
      "key_file": "/root/alencar/ensepro/ensepro/configuracoes/kg.key",
      "servicos": {
        "search": ":search"
      }
    }
  },
  "logger": {
    "nome_arquivo": "/ensepro.log",
    "modo_arquivo": "w",
    "formato": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "default_logger_nivel": "DEBUG",
    "modulos": {
      "arvore": {
        "nivel": "DEBUG"
      },
      "cbc": {
        "nivel": "DEBUG",
        "answer_generator": {
          "nivel": "DEBUG",
